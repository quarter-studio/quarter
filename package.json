    "packages/*"
  ],
  "scripts": {
    "build": "lerna run build",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "publish": "lerna publish"
  },
  "devDependencies": {
    "lerna": "^4.0.0",
